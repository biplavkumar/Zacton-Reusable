pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps {
                echo 'checking it out from the SCM ...'
            }
        }
      stage("build") {
            steps {
                echo 'Building the project......'
            }
        }
      stage("Test") {
            steps {
                echo 'Testing the project .....'
            }
        }
    }   
}
